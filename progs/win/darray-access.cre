program

	var x : ^int;
	var y : int;
	var f : float;
	var g : float;
	
begin

	y := 0;
	x := malloc (array[0 .. 7] of int);
	
	for y := 0 to 7 do begin
		write (y % 2);
		if y % 2 = 0 then x[y] := y * 10 + 7;
		else x[y] := y;
	end
	
	for y := 0 to 7 do write (x[y]);

	f := 156354.4;
	g := 36783.24;
	
	write (f);
	write (g);
	write (f / g);
	write (f % g);

end
