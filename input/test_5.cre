program

   var x : int;
   var fib0 : int;
   var fib1 : int;
   var fib : int;
   var i : int

begin
   x := 40;
   fib0 := 0;
   fib1 := 1;

   if (x=0) then
      fib := fib0
   else 
   	begin
      if (x=1) then
         fib := fib1
      else begin end
   	end;

   for i:=2 to x do
   	begin
      fib := fib0 + fib1;
      fib0 := fib1;
      fib1 := fib
   	end;

    write(fib)
end
